{
    "test_page": "tests/unit/all-tests.html",
    "port": 7000,    
    "parallel": 5,
    "timeout": 600,
    "report_file": "report.tap",
    "disable_watching": true,
    "on_start": "./tests/shared/js/saucie-connect.js",
    "on_exit": "./tests/shared/js/saucie-disconnect.js",
    "browser_start_timeout": 90,
    "launchers": {
        "SL_Chrome": {
            "exe": "./node_modules/saucie/lib/bin.js",
            "args": ["-b", "chrome", "-p", "Windows 10", "-v", "latest", "--no-ct", "--at", "-u"],
            "protocol": "browser"
        },
        "SL_Firefox": {
            "exe": "./node_modules/saucie/lib/bin.js",
            "args": ["-b", "firefox", "-p", "Windows 10", "-v", "latest", "--no-ct", "--at", "-u"],
            "protocol": "browser"
        },
        "SL_MS_Edge": {
            "exe": "./node_modules/saucie/lib/bin.js",
            "args": ["-b", "microsoftedge", "--no-ct", "--at", "-u"],
            "protocol": "browser"
        },
        "SL_Safari": {
            "exe": "./node_modules/saucie/lib/bin.js",
            "args": ["-b", "safari", "-v", "9", "--no-ct", "--at", "-u"],
            "protocol": "browser"
        }
    },
    "launch_in_ci": [
        "SL_Chrome",
        "SL_Firefox",
        "SL_MS_Edge",
        "SL_Safari"
    ]
}
